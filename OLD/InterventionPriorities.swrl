    (?bldg :façadeInsulationLevel "NONE")
    product(?r, 1.5, ?v)
-> (?priority rdf:type rscen:Priority) (?priority rscen:project ?p) (?priority rscen:post rscen:FaçadeInsulation) (?priority rscen:value ?v)]

[façade_low:
    (?p rdf:type rscen:RenoProject) (?p :onBuilding ?bldg)
    (?bldg :weightFaçade ?r)
    (?bldg :façadeInsulationLevel "LOW")
    product(?r, 1, ?v)
-> (?priority rdf:type rscen:Priority) (?priority rscen:project ?p) (?priority rscen:post rscen:FaçadeInsulation) (?priority rscen:value ?v)]

[façade_medium:
    (?p rdf:type rscen:RenoProject) (?p :onBuilding ?bldg)
    (?bldg :weightFaçade ?r)
    (?bldg :façadeInsulationLevel "MEDIUM")
    product(?r, 0.5, ?v)
-> (?priority rdf:type rscen:Priority) (?priority rscen:project ?p) (?priority rscen:post rscen:FaçadeInsulation) (?priority rscen:value ?v)]

