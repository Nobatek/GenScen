# baseURI: https://nobatek.inef4.com/renovation/building#
# prefix: bldg

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <https://nobatek.inef4.com/renovation/building#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


:Building
    rdf:type owl:Class .
  
:Facade
    rdf:type owl:Class .
    
:area
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Facade ;
    rdfs:range xsd:double .
	
:maxFacadeArea
	rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:double .
    
:glazingRatio
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:double .
    
:orientation
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Facade ;
    rdfs:range :Orientation .
    
:hasFacade
    rdf:type owl:ObjectProperty ;
    rdfs:domain :Building ;
    rdfs:range :Facade .

:roofArea
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:double .
    
:heatingEnergy
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range :Energy .
    
:centralHeating
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:boolean .

:centralDHW
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:boolean .

:ventilation
    rdf:type owl:ObjectProperty ;
    rdfs:domain :Building ;
    rdfs:range :Ventilation .
    
:glazingLevel
    rdf:type owl:ObjectProperty ;
    rdfs:domain :Building ;
    rdfs:range :WindowGlazing .
    
:ratioPV_Roof
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:double .

:facadeInsulation
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Facade ;
    rdfs:range :InsulationLevel .

:roofInsulation
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range :InsulationLevel .

:nbDwellings
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:integer .

:livingArea
    rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building ;
    rdfs:range xsd:double .

:Ventilation
  rdf:type owl:Class ;
  rdfs:comment "The type of ventilation installed." ;
  owl:oneOf (
    "None"
    "Mechanical"
    "Single hygro-regulated"
    "Dual flow"
  ) ;
.

:InsulationLevel
  rdf:type owl:Class ;
  rdfs:comment "The level of insulation of the facade. Values are None, Low or High." ;
  owl:oneOf (
    "None"
    "Low"
    "Medium"
    "High"
  ) ;
.

:WindowGlazing
  rdf:type owl:Class ;
  rdfs:comment "The type of glazing on the windows." ;
  owl:oneOf (
	"Single"
	"Double"
	"Triple"
  ) ;
.

:Energy
  rdf:type owl:Class ;
  rdfs:comment "The type of energy used for heating." ;
  owl:oneOf (
      "Electricity"
      "Gaz"
      "Fuel"
      "Wood"
      "Other"
    ) ;
.

:Orientation
  rdf:type owl:Class ;
  rdfs:comment "Orientation of a facade." ;
  owl:oneOf (
      "North" "North East" "North West" 
      "South" "South East" "South West"
      "West"
      "East"
    ) ;
.
