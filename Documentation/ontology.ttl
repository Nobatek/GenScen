#################################################################
#    Object Properties
#################################################################

###  https://nobatek.inef4.com/data-models/renovation-scenario#forProject
:forProject rdf:type owl:ObjectProperty ;
            rdfs:domain :Criterion ,
                        :RenoScenario ;
            rdfs:range :RenoProject ;
            rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#isMadeOf
:isMadeOf rdf:type owl:ObjectProperty ;
          rdfs:domain :RenoScenario ;
          rdfs:range :RenoAction ;
          rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#refines
:refines rdf:type owl:ObjectProperty ;
         rdfs:domain :RenoAlternative ,
                     :RenoApproach ;
         rdfs:range :RenoAction ,
                    :RenoAlternative ;
         rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


#################################################################
#    Data properties
#################################################################

###  https://nobatek.inef4.com/data-models/renovation-scenario#value
:value rdf:type owl:DatatypeProperty ;
       rdfs:domain :Criterion .


#################################################################
#    Classes
#################################################################

###  http://purl.org/vocommons/voaf#Vocabulary
<http://purl.org/vocommons/voaf#Vocabulary> rdf:type owl:Class .


###  http://www.w3.org/2006/vcard/ns#Individual
<http://www.w3.org/2006/vcard/ns#Individual> rdf:type owl:Class .


###  https://nobatek.inef4.com/data-models/renovation-scenario#BaselineScenario
:BaselineScenario rdf:type owl:Class ;
                  rdfs:subClassOf :RenoScenario .


###  https://nobatek.inef4.com/data-models/renovation-scenario#BuildingStatus
:BuildingStatus rdf:type owl:Class ;
                rdfs:subClassOf :Criterion ;
                owl:disjointWith :ClimateConditions ,
                                 :FinancialConstraint ;
                rdfs:comment "A specific type of generation criterion, related to the status of the building. It encompasses all kind of information related to the existing building, prior renovation." ;
                rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#ClimateConditions
:ClimateConditions rdf:type owl:Class ;
                   rdfs:subClassOf :Criterion ;
                   owl:disjointWith :FinancialConstraint ;
                   rdfs:comment "A specific type of generation criterion, related to the status of the building. It encompasses all kind of information related to the existing building, prior renovation." ;
                   rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#Criterion
:Criterion rdf:type owl:Class ;
           owl:disjointWith :RenoAction ,
                            :RenoAlternative ,
                            :RenoApproach ,
                            :RenoProject ,
                            :RenoScenario ;
           rdfs:comment "A criterion is a parameter of influence in the generation of a renovation scenario." ;
           rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#FinancialConstraint
:FinancialConstraint rdf:type owl:Class ;
                     rdfs:subClassOf :Criterion ;
                     rdfs:comment "A specific type of generation criterion, related to the economic constraints of the project. Typically, the global budget of the project is a driver for a renovation project." ;
                     rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#RenoAction
:RenoAction rdf:type owl:Class ;
            owl:disjointWith :RenoAlternative ,
                             :RenoApproach ,
                             :RenoProject ,
                             :RenoScenario ;
            rdfs:comment "A renovation action is a refinement of a renovation alternative, with technical details on the elements to associate to the renovation; ex: external wall insulation with 14cm mineral wool." ;
            rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#RenoAlternative
:RenoAlternative rdf:type owl:Class ;
                 owl:disjointWith :RenoApproach ,
                                  :RenoProject ,
                                  :RenoScenario ;
                 rdfs:comment "A renovation alternative is a refinement of a renovation approach, describing into more details the type of renovation action that will be made; ex: external/internal wall insulation..." ;
                 rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#RenoApproach
:RenoApproach rdf:type owl:Class ;
              owl:disjointWith :RenoProject ,
                               :RenoScenario ;
              rdfs:comment "A renovation approach describes the type of work that can be executed. It must be related to a some building element/renovation post; ex: insulation, windows, doors..." ;
              rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#RenoProject
:RenoProject rdf:type owl:Class ;
             owl:disjointWith :RenoScenario ;
             rdfs:comment "A renovation project is associated to one or many buildings, and can comprise many renovation scenarios." ;
             rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#RenoScenario
:RenoScenario rdf:type owl:Class ;
              rdfs:comment "A renovation scenario is a way to envisage the renovation of a/many buildings. It is made of multiple renovation approaches." ;
              rdfs:isDefinedBy <https://nobatek.inef4.com/data-models/renovation-scenario#> .


###  https://nobatek.inef4.com/data-models/renovation-scenario#nZeBScenario
:nZeBScenario rdf:type owl:Class ;
              rdfs:subClassOf :RenoScenario .


###  https://schema.org/Person
<https://schema.org/Person> rdf:type owl:Class .


